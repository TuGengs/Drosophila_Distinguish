            <solid android:color="#ffa8abad" />  
            <corners android:topLeftRadius="0.1dp" android:topRightRadius="0.1dp" 
                android:bottomRightRadius="10dp" android:bottomLeftRadius="10dp" />  
            <stroke android:width="1dp" android:color="#ffa8abad" />  
        </shape>  
    </item>
</layer-list>
